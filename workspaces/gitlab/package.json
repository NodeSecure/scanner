  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodeSecure/scanner.git"
  },
  "bugs": {
    "url": "https://github.com/NodeSecure/scanner/issues"
  },
  "homepage": "https://github.com/NodeSecure/scanner/tree/master/workspaces/gitlab#readme",
  "dependencies": {
    "@openally/httpie": "^1.0.0",
    "tar-fs": "^3.0.6"
  },
  "devDependencies": {
    "@slimio/is": "^2.0.0",
    "@types/tar-fs": "^2.0.4"
  },
  "engines": {
    "node": ">=20"
  }
