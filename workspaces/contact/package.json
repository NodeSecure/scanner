{
  "name": "@nodesecure/contact",
  "version": "3.0.0",
  "description": "Utilities to extract/fetch data on NPM contacts (author, maintainers ..)",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -b",
    "prepublishOnly": "npm run build",
    "test-only": "node --test ./test/**/*.spec.ts",
    "test-types": "npm run build && tsd && attw --pack . --profile esm-only",
    "test": "c8 -r html npm run test-only && npm run test-types"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "author",
    "contact",
    "maintainer"
  ],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodeSecure/scanner.git"
  },
  "bugs": {
    "url": "https://github.com/NodeSecure/scanner/issues"
  },
  "homepage": "https://github.com/NodeSecure/tree/master/workspaces/contact#readme",
  "devDependencies": {
    "@faker-js/faker": "^10.0.0"
  },
  "dependencies": {
    "@nodesecure/npm-types": "^1.2.0",
    "type-fest": "^5.0.1"
  },
  "tsd": {
    "directory": "test/types"
  }
}
